{
	"name": "crackerjackdigital/silverstripe-opensemanticsearch",
	"type": "silverstripe-module",
	"description": "Open Semantic Search tools for SilverStripe",
	"homepage": "http://github.com/crackerjackdigital",
	"license": "BSD-3-Clause",
	"minimum-stability": "dev",
	"keywords": [
		"silverstripe",
		"opensemanticsearch",
		"solr",
		"search",
		"fulltext",
		"documents",
		"index"
	],
	"authors": [
		{
			"name": "Steve Wake",
			"homepage": "https://github.com/wakes"
		},
		{
			"name": "CrackerJack Digital",
			"homepage": "http://www.crackerjack.net.nz/"
		}
	],
	"require": {
		"php": ">=5.4",
		"silverstripe/framework": ">=3.1",
		"solarium/solarium": "~3.8"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/CrackerjackDigital/silverstripe-opensemanticsearch.git"
		}
	],
	"extra": {
		"installer-name": "opensemanticsearch"
	},
	"autoload": {
		"psr-0": {
			"OpenSemanticSearch\\http": "code/traits",
			"OpenSemanticSearch\\json": "code/traits",
			"OpenSemanticSearch\\array_access": "code/traits",
			"OpenSemanticSearch\\array_bitfield_map": "code/traits"
		}
	}
}
